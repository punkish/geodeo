{
  "name": "geodeo",
  "scripts": {
    "dev": "bun run --hot ./src/index.js",
    "compile-linux": "bun build --compile --minify --sourcemap --target=bun-linux-x64-modern ./src/index.js --outfile ./bin/geodeox",
    "compile-mac": "bun build --compile --minify --sourcemap --target=bun-darwin-arm64 ./src/index.js --outfile ./bin/geodeox-mac"
  },
  "dependencies": {
    "geojson-vt": "^4.0.2",
    "vt-pbf": "^3.1.3"
  },
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  }
}
